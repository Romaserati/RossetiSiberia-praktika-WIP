@model IEnumerable<Customer>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Логин
            </th>
            <th>
                Фамилия
            </th>
            <th>
                Имя
            </th>
            <th>
                Отчество
            </th>
            <th>
                Номер телефона
            </th>
            <th>
                Email
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var user in Model)
        {
            <tr>
                <td>
                    @user.UserName
                </td>
                <td>
                    @user.LastName
                </td>
                <td>
                    @user.FirstName
                </td>
                <td>
                    @user.MiddleName
                </td>
                <td>
                    @user.PhoneNumber
                </td>
                <td>
                    @user.Email
                </td>
            </tr>
        }
    </tbody>
</table>

